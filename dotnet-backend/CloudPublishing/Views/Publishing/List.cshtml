@model IEnumerable<CloudPublishing.Models.Publishings.Entities.Publishing> 
@{
    ViewBag.Title = "Publishings";
}

<table>
    <tr>
        <th>Название</th>
        <th>Тип</th>
        <th>Тематика</th>
        <th>Рубрики</th>
    </tr>
    @foreach (var publishing in Model)
    {
    <tr>
        <td>@publishing.Title</td>
        <td>@publishing.Type</td>
        <td>@publishing.Subjects</td>
        <td>
            @foreach(var topic in publishing.Topics)
            {
                @(topic.Name + ", ");
            }
        </td>
    </tr>
    }
</table>
